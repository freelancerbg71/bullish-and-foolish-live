{
  "name": "bullish-and-foolish",
  "version": "1.30.0",
  "type": "module",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build:going-concern": "node scripts/generate_going_concern.js",
    "build:going-concern:sec": "node scripts/build_going_concern_from_sec.js --batchSize 50",
    "worker:daily-last-trade": "node worker/jobs/daily-last-trade.js",
    "ingest:companyfacts:test": "node scripts/ingest_companyfacts_dir.js --limit 20 --concurrency 1",
    "test:prices": "node server/prices/priceStore.test.js",
    "test:engine": "node engine/tests/utils.test.js && node engine/tests/rules.test.js",
    "engine:demo": "node engine/cli.js",
    "test": "npm run test:engine && npm run test:prices",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "security:scan": "snyk test",
    "security:monitor": "snyk monitor"
  },
  "dependencies": {
    "better-sqlite3": "^12.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0"
  }
}
